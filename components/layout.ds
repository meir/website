
str style = use("assets/main.scss")
func navigation = use("components/navigation")

return (str current_url, str title, element body) element {
  return : html {
    head {
      title > @{title}
      link rel="stylesheet" href=@{style};
    }
    body {
      @{navigation(current_url)}
      @{body}
    }
  }
}
